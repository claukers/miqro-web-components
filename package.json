{
  "name": "@miqro/web-components",
  "version": "0.0.1",
  "description": "helpers for creating dynamic HTMLElements with a basic template system",
  "type": "commonjs",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rm -Rf dist/;",
    "build": "tsc && tsc -p tsconfig-esm.json && webpack -c webpack.config-bundle-global.js",
    "pretest": "npm run build",
    "test": "LOG_LEVEL=debug NODE_ENV=test miqro-test test/*.test.js -i -n ",
    "predemo:serve": "npm run build && webpack -c webpack.config-demo-bundle.js",
    "demo:serve": "LOG_LEVEL=debug NODE_ENV=test DEMO_API_PORT=9696 node test/data/app/server/server.js",
    "demo:watch": "miqro watch test/data/app/front/src/ \"npm run predemo:serve\" & npm run demo:serve"
  },
  "author": "claukers",
  "license": "ISC",
  "devDependencies": {
    "@miqro/core": "^1.5.13",
    "@miqro/test": "^0.0.12",
    "miqro": "^1.7.16",
    "typescript": "4.6.3",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  }
}
