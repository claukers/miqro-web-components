{
  "name": "@miqro/web-components",
  "version": "0.0.1",
  "description": "helpers for creating dynamic HTMLElements with a basic template system",
  "type": "commonjs",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rm -Rf dist/;",
    "build": "tsc && tsc -p tsconfig-esm.json",
    "postbuild": "deno bundle dist/esm/index.js dist/esm.bundle.js && cp dist/esm.bundle.js dist/esm.bundle.mjs",
    "pretest": "npm run build",
    "test": "LOG_LEVEL=debug NODE_ENV=test miqro-test -r src/ -i -n "
  },
  "author": "claukers",
  "license": "ISC",
  "devDependencies": {
    "@miqro/test": "0.2.4",
    "miqro": "1.8.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
  }
}
