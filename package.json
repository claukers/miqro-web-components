{
  "name": "@miqro/web-components",
  "version": "0.0.1",
  "description": "helpers for creating dynamic HTMLElements with a basic template system",
  "type": "commonjs",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rm -Rf dist/;",
    "tsc": "npm run prebuild && tsc",
    "build": "tsc && tsc -p tsconfig-esm.json",
    "pretest": "tsc",
    "test": "LOG_LEVEL=debug NODE_ENV=test miqro-test -r src/ -i -n "
  },
  "author": "claukers",
  "license": "ISC",
  "devDependencies": {
    "@miqro/core": "^1.5.13",
    "@miqro/test": "^0.0.12",
    "miqro": "^1.7.16",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  }
}
